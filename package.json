{
  "name": "freeter.io",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/FreeterApp/freeter.io.git",
  "author": "Alex Kaul",
  "license": "CC-BY-SA-4.0",
  "scripts": {
    "dev:pages": "npx eleventy --watch",
    "dev:scripts": "webpack --mode=development --watch",
    "dev:styles": "sass --embed-source-map --watch src/styles:build/styles",
    "dev": "rimraf build && concurrently --names \"STYLES,SCRIPTS,PAGES\" -c \"blue.bold,magenta.bold,green.bold,yellow.bold\" \"yarn run dev:styles\" \"yarn run dev:scripts\" \"yarn run dev:pages\"",
    "prod:pages": "npx eleventy",
    "prod:scripts": "webpack --mode=production",
    "prod:styles": "sass --no-source-map src/styles:build/styles && postcss ./build/styles/*.css --replace",
    "prod": "rimraf build && concurrently --names \"STYLES,SCRIPTS,PAGES\" -c \"blue.bold,magenta.bold,green.bold,yellow.bold\" \"yarn run prod:styles\" \"yarn run prod:scripts\" \"yarn run prod:pages\""
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.3",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.7",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {}
}
